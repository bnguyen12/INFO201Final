library("dplyr")
library("shiny")
library("ggplot2")
age.data <- read.csv('./data/AgeViewBySeason.csv', stringsAsFactors = FALSE)
season.12.thru.17 <- age.data[1:24, ]
season.05.thru.11 <- age.data[25:52, ]
season.04.thru.97 <- age.data[53:84, ]
season.data <- mutate(season.04.thru.97, "Type_A" = rowSums(season.data[, 3:7]),
"Type_B" = rowSums(season.data[, 8:10]),
"Type_H3N2v" = H3N2v) %>%
select(Season, Age.Group, Type_A, Type_B, Type_H3N2v)
season.data <- mutate(season.04.thru.97, "Type_A" = rowSums(season.04.thru.97[, 3:7]),
"Type_B" = rowSums(season.04.thru.97[, 8:10]),
"Type_H3N2v" = H3N2v) %>%
select(Season, Age.Group, Type_A, Type_B, Type_H3N2v)
View(season.data)
ggplot() +
geom_line(data = season.data, mapping = aes(x = Age.Group, y = Type_A), fill = Season)
View(season.data)
